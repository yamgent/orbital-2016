<%= javascript_tag do %>

  <% @user.courses.each do |c| %>
    <% c.tutorials.each do |tut| %>
      Timetable().addPeriod(
        <%= tut.id %>,
        <%= tut.day %>,
        <%= getHourAndMinAsInt(tut.start_time) %>,
        <%= getHourAndMinAsInt(tut.end_time) %>,
        "<%= c.code %>",
        "<%= c.name %>",
        "Tutorial"
      ).togglePeriodStyle(<%= tut.id %>, "faded");
    <% end %>
  <% end %>
  
<% end %>
