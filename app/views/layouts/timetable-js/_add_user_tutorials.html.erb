<%= javascript_tag do %>

  <% @user.tutorials.each do |tut| %>
    Timetable().initPeriod(
      <%= tut.id %>,
      <%= tut.day %>,
      <%= getHourAndMinAsInt(tut.start_time) %>,
      <%= getHourAndMinAsInt(tut.end_time) %>,
      "<%= tut.course.code %>",
      "<%= tut.course.name %>",
      PeriodTypes.TUTORIAL,
      "<%= tut.group_number %>"
    )
    .showPeriod(<%= tut.id %>)
    .addPeriodStyle(<%= tut.id %>, "selected", 0);
  <% end %>

<% end %>
