<%= javascript_tag do %>

  <% @user.tutorials.each do |tut| %>
    Timetable().addPeriod(
      <%= tut.id %>,
      <%= tut.day %>,
      <%= getHourAndMinAsInt(tut.start_time) %>,
      <%= getHourAndMinAsInt(tut.end_time) %>,
      "<%= tut.course.code %>",
      "<%= tut.course.name %>",
      "Tutorial"
    ).togglePeriodStyle(<%= tut.id %>, "selected");
  <% end %>

<% end %>
