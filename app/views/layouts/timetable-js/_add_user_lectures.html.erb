<%= javascript_tag do %>

  <% @user.courses.each do |c| %>
    <% c.lectures.each do |lec|  %>
      Timetable().initPeriod(
        "lecture-<%= lec.id %>",
        <%= lec.day %>,
        <%= getHourAndMinAsInt(lec.start_time) %>,
        <%= getHourAndMinAsInt(lec.end_time) %>,
        "<%= c.code %>",
        "<%= c.name %>",
        PeriodTypes.LECTURE
      )
      .showPeriod("lecture-<%= lec.id %>")
      .addPeriodStyle("lecture-<%= lec.id %>", "selected", 0);
    <% end %>
  <% end %>

<% end %>
